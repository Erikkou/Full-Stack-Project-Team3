{
    "name": "my_project/backend",
    "description": "Symfony backend for scori2 project",
    "version": "1.0.0",
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^5.4",
        "symfony/console": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/routing": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/runtime": "^5.4",
        "symfony/yaml": "^5.4",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^2.20"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.50",
        "phpunit/phpunit": "^9.5"
    }
}
